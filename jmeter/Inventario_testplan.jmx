          <boolProp name="resetInterpreter">false</boolProp>
          <stringProp name="script">import java.util.Base64;
import java.security.SecureRandom;

// Generar valores aleatorios
int userId = (int)(Math.random() * 10000) + 1;  // ID 1-10000
String[] roles = {&quot;user&quot;, &quot;admin&quot;, &quot;guest&quot;, &quot;manager&quot;};
String role = roles[(int)(Math.random() * roles.length)];

// Timestamp actual
long now = System.currentTimeMillis() / 1000L;

// Crear header JWT
String header = &quot;{\&quot;alg\&quot;:\&quot;HS256\&quot;,\&quot;typ\&quot;:\&quot;JWT\&quot;}&quot;;
String encodedHeader = Base64.getEncoder().encodeToString(header.getBytes());

// Crear payload aleatorio
String payload = &quot;{\&quot;sub\&quot;:\&quot;&quot; + userId + &quot;\&quot;,\&quot;role\&quot;:\&quot;&quot; + role + &quot;\&quot;,\&quot;iat\&quot;:&quot; + now + &quot;,\&quot;exp\&quot;:&quot; + (now + 3600) + &quot;}&quot;;
String encodedPayload = Base64.getEncoder().encodeToString(payload.getBytes());

// Signature aleatoria
byte[] randomBytes = new byte[32];
new SecureRandom().nextBytes(randomBytes);
String signature = Base64.getEncoder().encodeToString(randomBytes);

// JWT completo
String jwt = encodedHeader + &quot;.&quot; + encodedPayload + &quot;.&quot; + signature;

// Guardar en variable
vars.put(&quot;random_token&quot;, jwt);

// Log para debug
log.info(&quot;Generated token for user: &quot; + userId + &quot;, role: &quot; + role);</stringProp>
